<script>
    var countryCode;
    $.ajax( {
        url: '//geoip.nekudo.com/api',
        success: function(response) {
            if (response) {
                if (response.country) {
                    if (response.country.code) {
                        countryCode = response.country.code;
                    }

                    if (response.country.name) {
                        $('.country-name').html('to ' + response.country.name);

                        if (response.city) {
                            $('.city-country-name').html('to ' + response.city + ', ' + response.country.name);    
                        } else {
                            $('.city-country-name').html('to ' + response.country.name);
                        }
                    }
                }
            }
        }
    });
</script>

