<script>
  $(document).ready( function() {
      $('.slideout-carousel').on('touchmove', function(e){ e.preventDefault(); })
    
      /* handle events to load quickView specific to slide cart */
      $('.fn-cart-slide-modal-quick').click(function(){
          var locat = $(this).attr('data-href');
      	  //$("#slideCartQuickViewContainer").html('<object data="'+ locat +'">');
          $("#slideCartQuickViewContainer").find('.cart-quick-view-modal-content-container').load(locat, function(){
              $('#slideCartQuickViewContainer').addClass('cart-slide-quick-view-opened').fadeIn();
              $('.fn-close-modal-quick-view-cart').click(function() {
                  $("#slideCartQuickViewContainer").removeClass('cart-slide-quick-view-opened').fadeOut();
              });
          });
      });
  });
</script>
    </div>
</div>

<script id="slideCartTemplate" type="text/x-handlebars-template">
	<div class="items-preview-wrapper">
        <div class="cart-viewport">
        	<h3 class="empty-cart" style="display: none">Your Cart is Empty</h3>
            {{#each items}}
            <div class="item-wrapper">
                <div class="item-image">
                    <a href="{{ this.url }}" title="View {{ this.title }}">
                        <img src="{{ this.image }}" alt="{{ this.title }}" />
                    </a>
                </div>
                <div class="item-description">
                    <a classs="item-name" href="{{ this.url }}" title="View {{ this.title }}">
                        {{ this.title }}{{#if this.variantProduct }} - {{ this.variantProduct }}{{/if}} x{{ this.quantity }}
                    </a>
                    <h3 class="item-price">
                    	<span class="money">{{ this.price }} </span>{{#if this.multiple}}<span class="small"> /each</span>{{/if}}
                    </h3>
                    <a class="preview-cart-item-delete-button" data-index="{{ @index }}" data-href="/cart/change?line={{ @index }}&amp;quantity=0" data-id="{{this.id}}"> <i class="fa fa-times" ></i> Remove item</a>
                </div>

            </div>
            {{/each}}
	  </div>
  </div>
  <div id="slide-cart-total-final-price" class="cart-prices">
      
  	  <div class="shipping clearfix">
          <span class="shipping-text txt">Free Shipping</span>
          <span class="price-wrapper price">$0.00 USD</span>
  	  </div>

	  <div class="total clearfix">
      	  <span class="price-text txt">Total</span>
          <span class="price-wrapper price fn-discountedTotalAmount">{{ total }} USD</span>
  	  </div>
  </div>
</script>

<script>
  function deleteFromSlideCart(id) {
      Shopify.api.changeItem(id, 0, function(c){
          $('.cart-item-count').text(c.item_count);
          $('.fn-cart-item-count').text(c.item_count);
          $('.fn-cart-item-count').attr('data-items', c.item_count);
          updateSlideCart();
      });
  }
  
  function updateSlideCart() {
      $.getJSON('/cart.js', function(cart) {
          var source = $("#slideCartTemplate").html();
          var template = Handlebars.compile(source);
          var items = [];
          var total = Shopify.api.formatMoney(cart.total_price);

          $.each(cart.items, function(i,o){
              var multiples = 0;
              
              if(o.quantity > 1) {
                 multiples = 1;
              }
            
              items.push({
                  multiple: multiples,
                  id: o.id,
                  title: o.product_title,
                  variantProduct: o.variant_title,
                  url: o.url,
                  quantity: o.quantity,
                  price: Shopify.api.formatMoney(o.price),
                  totalPrice: Shopify.api.formatMoney(o.price * o.quantity),
                  image: o.image.replace(/\.jpg/,"_small.jpg")
              });
          });
        
          $("#slideCartContent").html( template({items: items, total: total}) );
        
          if (items.length) {
              $(".empty-cart").hide();
          } else {
              $(".empty-cart").show();
          }

          $('.preview-cart-item-delete-button').unbind('click').click(function(){
              deleteFromSlideCart($(this).attr('data-id'));
              updateSlideCart();
          });
      });
  }
  
  
  $(document).ready(function(){
      $('.preview-cart-item-delete-button').click(function(){
          deleteFromSlideCart($(this).attr('data-id'));
      });
  });
</script>

<script>
function addItem(item, callback) {
    $.ajax({
        url: '/cart/add.js',
        type: 'POST',
        data: item,
        dataType: 'json',
        success: callback
    });
}

function cartUpdated(cart) {
    /*enable add button*/  
  	// this.$cart.removeClass('disabled').prop('disabled', false).html(window.theme.strings.addToCart);
    //$('.cart-item-count').text(cart.item_count);
  	$('.fn-cart-item-count').text(cart.item_count).attr('data-items', cart.item_count);
    //Llamada al update slider y le paso el cart
    updateSlideCart(cart);
}

function getCart(callback) {
    $.ajax({
        url: '/cart.json',
        type: 'GET',
        dataType: 'json',
        success: callback
    });
}

/*addItem(item, function(){
    getCart(cartUpdated);
});*/

/*var item = {
    id: $(''),
    quantity: 1
};*/
  
$('.purchasecartPreviewSlider').click(function() {
  	var that = this;
  	var prodId = $(this).parent().find('select').val();
  
	$(this).prop('disabled', true).addClass('disabled').html(CART_LOADING);
  
  	var item = {
    	id: prodId,
    	quantity: 1
  	};

  	/*addItem(item, function(){
      	$(that).prop('disabled', false).removeClass('disabled').html(window.theme.strings.addToCart);
    	getCart(cartUpdated);
    });*/
  
  	function addToCart() {
      	addItem(item, function(){
            $(that).prop('disabled', false).removeClass('disabled').html(window.theme.strings.addToCart);
            getCart(cartUpdated);
        });
  	}

  
  	function showSlideOutCart() {
		$(that).removeClass('disabled').prop('disabled', false).html(window.theme.strings.addToCart);
    }
  
  	getCart(function(cart) {
      	var items = cart.items.filter(function(item) {
        	return item.id === parseInt(prodId);
        });
      
      	var quantity = 0;
      
      	items.map(function(item) {
      		quantity += item.quantity;
        });
            
      	if (items.length) {
			var itemInCart = items[0];
        	//Fill duplicated Alert Modal
          	$('#duplicatedAlertModal #duplicated-items-to-add-count').html('1');
          	$('#duplicatedAlertModal #duplicated-product-image').attr('src', itemInCart.image);

          	$('#duplicatedAlertModal #duplicated-items-cart-count').html(quantity);
          	$('#duplicatedAlertModal #duplicated-items-title').html(itemInCart.title);
            $('#duplicatedAlertModal #duplicated-items-product-title').html(itemInCart.product_title);
            $('#duplicatedAlertModal #duplicated-items-product-variants').html(itemInCart.variant_title);

          	//Show Modal
          	$('#duplicatedAlertModal').modal('show');

            //Open Cart
          	$('slideCartQuickViewContainer').removeClass('cart-slide-quick-view-opened');
          
          	window.addToCart = addToCart;
            window.showSlideOutCart = showSlideOutCart;
        } else {
        	addToCart();
        }
    });
  
  	
});
</script>

<script src="//cdn.shopify.com/s/files/1/1865/8449/files/api.js?12244142076954982116" type="text/javascript"></script>
  
