            var counter = 1;

            function showDiv() {

              jQuery(".top-header").find(".cycle-container").each(function(){
                var data_ordinal = jQuery(this).attr("data-ordinal");

                if(data_ordinal == counter){
                  jQuery(this).fadeIn(1000);
                }else{
                  jQuery(this).css("display", "none");
                }
              });

              counter == 3? counter = 1 : counter++; 

            }
        }
  
