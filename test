<script>
    $(window).scroll(function(){
        var sticky = $('.sticky'),
        scroll = $(window).scrollTop();

        if (scroll >= 1) sticky.addClass('fixed');
        else sticky.removeClass('fixed');
    });
</script>


<script>
    $(window).scroll(function(){
        var stickyAnnouncementBar = $('.stickyAnnouncementBar'),
        scroll = $(window).scrollTop();

        if (scroll >= 1) stickyAnnouncementBar.addClass('fixed');
        else stickyAnnouncementBar.removeClass('fixed');
    });
</script>

