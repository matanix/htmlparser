  <script>
    $(function() {
      $("img.lazy").lazyload({
        effect : "fadeIn"     
      });
    });
  </script>
  
 <script>
function getQueryStringParameter(parameter) {
    var i;
    var keyValueParameter;
    var parameterValue;

    var keyValueParametersArray = window.location.search.substring(1).split('&');

    for (i = 0; i < keyValueParametersArray.length; i++) {
        keyValueParameter = keyValueParametersArray[i].split('=');

        if (keyValueParameter[0] === parameter) {
            parameterValue = keyValueParameter[1];
        }
    }

    return parameterValue;
}

var productHandle = getQueryStringParameter('itemId');

var variantId = getQueryStringParameter('variant');

var allProducts = [];

var scrollEnabled;

//var hasProductInParams = false;

var collectionPaginatedProductsBaseUrl = window.location.pathname + '/products';

var isLoading = false;

var itemCounter = 0;

function getClassPosition(item_position) {
    var item_classes = '';
    switch (itemCounter) {
        case 0:
        case 4:
        case 8:
            item_classes = "alpha mobile-clear";
            break;
        case 1:
        case 5:
        case 9:
            item_classes = "";
            break;
        case 2:
        case 6:
        case 10:
            item_classes = "mobile-clear";
            break;
        case 3:
        case 7:
        case 11:
            item_classes = "omega";
            break;
    }
    itemCounter += 1;
    if (itemCounter == 4) {
        itemCounter = 0;
    } else {

    }
    return item_classes;
}

var myInterval;

var itemPosition = 1;

function drawItems() {
    var source = $('#productPaginationTemplate').html();
    var template = Handlebars.compile(source);

    if (!allProducts.length) {
        $(window).unbind('scroll', drawItems);
    } else {
        $('.products-wrapper').append(template({ items: allProducts.splice(0, paginate.page_size) }));
    }

    bindProductReviews();
    if (window.hasLikesInfo && $('.likeCheckBox, .quick-likeCheckBox').not('[binded="true"]').length) {
        bindProductLikes();
    }
}

function drawItemsPreloaded() {
    if (allProducts.length < paginate.page_size && paginate.current_page + 1 <= paginate.pages) {
        paginateItems();
    } else {
        drawItems();
    }
}

/* Load FirstItem If param or load the first pagination into an object */
function loadFirstItem(prod, variant) {
    if (prod) {
        var source = $('#productPaginationTemplate').html();
        var template = Handlebars.compile(source);
        var items = [];
        var item;

        var targetItemName = prod;

        $.ajax({
            type: 'GET',
            url: '/products/' + targetItemName + '.js',
            dataType: 'json',
            success: function(response) {
                var product;

                var splittedImage;
                var product;
                var priceArray;
                var compareAtPriceArray;
                var availableArray;
                var variantIndex;

                var varImage;

                item = {};

                product = response;

                priceArray = [];
                compareAtPriceArray = [];
                availableArray = [];

                if (product.tags.indexOf('hidden') == -1) {
                    if (variant) {
                        /* Get elements from variants array prices AND images */
                        for (variantIndex in product.variants) {
                            priceArray.push(parseFloat(product.variants[variantIndex].price).toFixed(2));
                            compareAtPriceArray.push(parseFloat(product.variants[variantIndex].compare_at_price).toFixed(2));

                            /* Obtain variant image*/
                            if (product.variants[variantIndex].id == variant) {
                                if (product.variants[variantIndex].featured_image) {
                                	varImage = product.variants[variantIndex].featured_image.src;
                                } else {
                                  	varImage = product.featured_image;
                                }
                            }
                            /*availableArray.push(product.variants[variantIndex].available);*/
                        }

                        /*var currentVariant = product.variants.find(function(productVariant){
                            return productVariant.id == variant;
                        });*/

                        if ($(splittedImage = product.images[0].split('.jpg')).length === 2) {
                            item.image = splittedImage[0] + '_large.jpg' + splittedImage[1];
                        } else if ($(splittedImage = product.images[0].split('.png')).length === 2) {
                            item.image = splittedImage[0] + '_large.png' + splittedImage[1];
                        } else {
                            item.image = product.images[0];
                        }

                        if (varImage) {
                            item.image = varImage;
                        } else {
                            item.image = product.images[0];
                        }
                    } else {
                        /* Get elements from variants array Only prices */
                        for (variantIndex in product.variants) {
                            priceArray.push(parseFloat(product.variants[variantIndex].price).toFixed(2));
                            compareAtPriceArray.push(parseFloat(product.variants[variantIndex].compare_at_price).toFixed(2));
                            /*availableArray.push(product.variants[variantIndex].available);*/
                        }


                        if ($(splittedImage = product.images[0].split('.jpg')).length === 2) {
                            item.image = splittedImage[0] + '_large.jpg' + splittedImage[1];
                        } else if ($(splittedImage = product.images[0].split('.png')).length === 2) {
                            item.image = splittedImage[0] + '_large.png' + splittedImage[1];
                        } else {
                            item.image = product.images[0];
                        }

                        item.image = product.images[0];
                    }



                    item.id = product.id;
                    item.title = product.title;
                    item.handle = product.handle;
                    //item.image = product.images[0].src;
                    item.available = product.available;

                    /* Special case of money Received Without points */
                    item.price_min = Shopify.api.formatMoney(Math.min.apply(null, priceArray).toString().replace('.', ''));
                    item.oldPrice = Shopify.api.formatMoney(Math.max.apply(null, compareAtPriceArray).toString().replace('.', ''));
                    item.price_varies = Math.min.apply(null, priceArray) !== Math.max.apply(null, priceArray);
                    item.url = collectionPaginatedProductsBaseUrl + '/' + product.handle;
                    item.discountPercentage = Math.round(((Math.max.apply(null, compareAtPriceArray) - Math.min.apply(null, priceArray)) / (Math.max.apply(null, compareAtPriceArray))) * 100);

                    item.position = getClassPosition(0);

                    allProducts.push(item);
                }
            },
            complete: function() {
                drawItemsPreloaded();
            }
        });
    } else {
        drawItemsPreloaded();
    }
}

function paginateItems() {
    if (paginate.current_page + 1 <= paginate.pages) {
        var scrollNode = $('.more').last();
        var loader = $('.loader-circle');
        var source = $('#productPaginationTemplate').html();
        var template = Handlebars.compile(source);
        var items = [];

        isLoading = true;

        //if (scrollNode.length > 0 && scrollNode.css('display') != 'none') {
        $.ajax({
            type: 'GET',
            url: collectionPaginatedProductsBaseUrl + '.json?',
            data: {
                limit: paginate.page_size,
                page: paginate.current_page + 1
            },
            beforeSend: function() {
                scrollNode.hide();
                loader.show();
            },
            success: function(collection) {
                var items = [];
                var splittedImage;
                var index;
                var product;
                var item;
                var priceArray;
                var compareAtPriceArray;
                var availableArray;
                var variantIndex;

                for (index in collection.products) {
                    priceArray = [];
                    compareAtPriceArray = [];
                    availableArray = [];
                    item = {};

                    product = collection.products[index];

                    /* Control hidden products on sales channel with the hidden tag */
                    if (product.tags.indexOf('hidden') == -1 && product.handle != productHandle) {

                        for (variantIndex in product.variants) {
                            priceArray.push(parseFloat(product.variants[variantIndex].price).toFixed(2));
                            compareAtPriceArray.push(parseFloat(product.variants[variantIndex].compare_at_price).toFixed(2));
                            availableArray.push(product.variants[variantIndex].available);
                        }

                        if ($(splittedImage = product.images[0].src.split('.jpg')).length === 2) {
                            item.image = splittedImage[0] + '_large.jpg' + splittedImage[1];
                        } else if ($(splittedImage = product.images[0].src.split('.png')).length === 2) {
                            item.image = splittedImage[0] + '_large.png' + splittedImage[1];
                        } else {
                            item.image = product.images[0].src;
                        }

                        item.id = product.id;
                        item.title = product.title;
                        item.handle = product.handle;
                        //item.image = product.images[0].src;
                        item.available = availableArray.indexOf(true) !== -1;
                        item.price_min = Shopify.api.formatMoney(Math.min.apply(null, priceArray).toFixed(2).toString().replace('.', ''));
                        item.oldPrice = Shopify.api.formatMoney(Math.max.apply(null, compareAtPriceArray).toFixed(2).toString().replace('.', ''));

                        item.price_varies = Math.min.apply(null, priceArray) !== Math.max.apply(null, priceArray);
                        item.url = collectionPaginatedProductsBaseUrl + '/' + product.handle;
                        item.discountPercentage = Math.round(((Math.max.apply(null, compareAtPriceArray) - Math.min.apply(null, priceArray)) / (Math.max.apply(null, compareAtPriceArray))) * 100);

                        item.position = getClassPosition(itemPosition);

                        itemPosition += 1;

                        allProducts.push(item);
                    }
                }
            },
            complete: function() {
                loader.hide();

                if (paginate.current_page === paginate.pages - 1) {
                    $(window).unbind('scroll', loadNextPage);
                    $(window).bind('scroll', drawItems);
                } else {
                    if (!scrollEnabled) {
                        enableScroll();
                    }

                    scrollNode.show();
                }

                paginate.current_page++;

                drawItemsPreloaded();

                isLoading = false;
            }
        })
        //}
    }
}

function executeScroll() {
    if (!isLoading) {
        if ($(window).scrollTop() + $(window).height() * 8 >= $(document).height()) {
            paginateItems();
        }
    }
}

function loadNextPage() {
    setTimeout(executeScroll, 200);
}

function enableScroll() {
    scrollEnabled = true;

    $(window).bind('scroll', loadNextPage);
}

$(document).ready(function() {
    if (collectionPaginatedProductsBaseUrl) {
        /*$(window).bind('scroll', loadNextPage);*/
        loadFirstItem(productHandle, variantId);

        //console.log(productHandle + '  ' +  variantId)
    } else {
        $('.more').last().hide();
    }
});
</script>




  
</div><!-- container-fluid -->



<script>
    function getReviewsRating(productIds) {
        $.ajax({
            method: 'GET',
            url: 'https://' + window.location.host + '/a/comment/get_summary_star_collection',
            data: {
                product_ids: productIds,
                shop_domain: 'bikinimas.myshopify.com',
                isAdminLogin: false
            },
            success: function(response) {
                var index;
                var id;
                var template;
                var totalReviewsTag;
                var ratingStars;
                var starsIndex;
                var avgStar;
                var totalReviews;

                for (index in productIds) {
                    id = productIds[index];

                    avgStar = response.avg_star[id];
                    totalReviews = response.total_review[id];

                    if (totalReviews) {
                        if (totalReviews === 1) {
                            totalReviewsTag = '1 REVIEW';
                        } else {
                            totalReviewsTag = totalReviews + ' REVIEWS';
                        }
                    } else {
                        totalReviewsTag = 'NO REVIEWS';
                    }

                    ratingStars = '';

                    for (starsIndex = 0; starsIndex < 5; starsIndex++) {
                        ratingStars += (
                            '<li' +
                                ' class="fa fa-star' + (starsIndex < Math.round(avgStar) ? '' : '-o') + '"' +
                                ' style="font-size: 10px; color: #fff; display: none"' +
                            '></li>'
                        );
                    }

                    template = (
                        '<div class="alireview-number-total-review" style="display: none;">' +
                            avgStar +
                        '</div>' +
                        '<div class="alireview-total-review-custom">' +
                            '<div class="alireview-star-custom" value="' + Math.round(avgStar) + '">' +
                                ratingStars +
                            '</div>' +
                            '<div class="alireview-total-text-custom" style="display: none;">' + totalReviewsTag + '</div>' +
                        '</div>'
                    );

                    $('.shopify-ali-review-rating[product-id="' + id + '"]').html(template);
                }
            }
        })
    }

    function bindProductReviews() {
        var productIds = [];

        jQuery('.shopify-ali-review-rating').not('[binded="true"]').attr('binded', true).each(function() {
            productIds.push($(this).attr('product-id'))
        });

        getReviewsRating(productIds);
    }

    bindProductReviews();
</script>

  

</div>

            
                  
            
			
				
		
		</div>
  

	
